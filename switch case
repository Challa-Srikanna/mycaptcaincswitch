#include <stdio.h>
int main()
{
     int num;
     printf("Enter a number to get your food item");
     scanf("%d",&num);
     switch(num)
     {
         case 1:
           printf("Pizza,Rs199");
         case 2:
           printf("Burger, Rs 129");
         case 3:
           printf("Pasta, Rs 179");
         case 4:
           printf("French Fries, Rs 99");
         case5:
           printf("Sandwich, Rs 149");
         default:
           printf("Not existing");
    }
    return 0;
}
